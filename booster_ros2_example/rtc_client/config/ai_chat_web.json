{
  "provider": "ragflow",
  "api_base": "https://YOUR-RAGFLOW-HOST",
  "agent_id": "YOUR_AGENT_ID",
  "ragflow_api_key": "YOUR_RAGFLOW_API_KEY",
  "openai_api_key": "YOUR_OPENAI_API_KEY",
  "model": "default",
  "agent_completion_path": "/api/v1/agents_openai/{agent_id}/chat/completions",
  "headers": {
    "Content-Type": "application/json"
  },
  "auth_header": "Authorization",
  "auth_prefix": "Bearer ",
  "request_timeout_sec": 120,
  "command_triggers": [
    {
      "phrase": "come here",
      "label": "come_here",
      "command": "python /opt/booster/booster_gym/deploy/deploy.py --config=k1_gesture_001.yaml --auto-start --duration=6",
      "case_sensitive": false
    }
  ],
  "enable_command_exec": false,
  "asr_model_path": "",
  "asr_sample_rate": 16000,
  "asr_chunk_ms": 200,
  "asr_max_sec": 12,
  "stt_backend": "openai",
  "stt_api_base": "https://api.openai.com",
  "stt_model": "gpt-4o-mini-transcribe",
  "stt_language": "en",
  "stt_prompt": "",
  "stt_sample_rate": 16000,
  "stt_max_sec": 10,
  "stt_timeout_sec": 120,
  "stt_trigger_phrase": "Hey Robo",
  "stt_trigger_required": true,
  "stt_trigger_timeout_sec": 30,
  "stt_silence_ms": 500,
  "stt_rms_threshold": 500,
  "stt_min_speech_ms": 200,
  "stt_log_rms": false,
  "loco_action_runner": "/home/booster/Workspace/booster_robotics_sdk/example/high_level/b1_loco_action_runner.py",
  "loco_interface": "lo",
  "enable_tts": true,
  "tts_backend": "openai",
  "tts_api_base": "https://api.openai.com",
  "tts_model": "tts-1",
  "tts_voice": "alloy",
  "tts_response_format": "wav",
  "tts_play_command": "aplay",
  "tts_keep_audio": false,
  "walk_step_sec": 1,
  "walk_magnitude": 1.0,
  "local_tts_api_base": "http://localhost:8880",
  "local_tts_api_key": "",
  "local_tts_model": "kokoro",
  "local_tts_voice": "af_heart"
}
